{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rulesDirectory": [
    "node_modules/vrsource-tslint-rules/rules"
  ],
  "rules": {
    "no-console": [true, "log"],
    "no-duplicate-imports": true,
    "no-duplicate-variable": true,
    "no-jasmine-focus": true,
    "no-var-keyword": true,
    "semicolon": [true],
    "variable-name": [true, "ban-keywords"],
    "no-inner-declarations": [true, "function"],
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false,
    "ter-indent": [
      true,
      2,
      {
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        },
        "SwitchCase": 1
      }
    ]
  },
  "jsRules": {
    "no-console": [true, "log"],
    "no-duplicate-imports": true,
    "no-duplicate-variable": true,
    "no-jasmine-focus": true,
    "semicolon": [true],
    "variable-name": [true, "ban-keywords"],
    "no-inner-declarations": [true, "function"],
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": false,
    "ter-indent": [
      true,
      2,
      {
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        },
        "SwitchCase": 1
      }
    ]
  }
}
